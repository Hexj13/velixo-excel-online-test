{
  "name": "excel-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-End testing for Excel Online using Playwright",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:debug": "npx playwright test --debug",
    "test:headed": "npx playwright test --headed",
    "allure": "allure generate ./allure-results --clean -o ./allure-report && allure open ./allure-report",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "cleanup": "rimraf allure-report allure-results auth test-results",
    "install:allure": "npm install -g allure-commandline"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "excel",
    "office365"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "allure-playwright": "^2.15.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.5.4"
  }
}